{
  "name": "mini-program",
  "version": "1.0.0",
  "private": true,
  "description": "妙瓦小程序源码",
  "templateInfo": {
    "name": "vue3-NutUI",
    "typescript": true,
    "css": "Sass",
    "framework": "Vue3"
  },
  "browserslist": [
    "last 3 versions",
    "Android >= 4.1",
    "ios >= 8"
  ],
  "scripts": {
    "build:weapp": "rimraf dist && cross-env NODE_ENV=production taro build --type weapp",
    "dev:weapp": "rimraf dist && taro build --type weapp --watch",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "build:swan": "taro build --type swan",
    "build:alipay": "taro build --type alipay",
    "build:tt": "taro build --type tt",
    "build:h5": "taro build --type h5",
    "build:rn": "taro build --type rn",
    "build:qq": "taro build --type qq",
    "build:quickapp": "taro build --type quickapp",
    "dev:swan": "npm run build:swan -- --watch",
    "dev:alipay": "npm run build:alipay -- --watch",
    "dev:tt": "npm run build:tt -- --watch",
    "dev:h5": "npm run build:h5 -- --watch",
    "dev:rn": "npm run build:rn -- --watch",
    "dev:qq": "npm run build:qq -- --watch",
    "dev:quickapp": "npm run build:quickapp -- --watch"
  },
  "dependencies": {
    "@babel/runtime": "^7.7.7",
    "@nutui/nutui-taro": "^4.2.8",
    "@tarojs/components": "4.1.7",
    "@tarojs/helper": "4.1.7",
    "@tarojs/plugin-framework-vue3": "4.1.7",
    "@tarojs/plugin-html": "4.1.7",
    "@tarojs/plugin-platform-alipay": "4.1.7",
    "@tarojs/plugin-platform-h5": "4.1.7",
    "@tarojs/plugin-platform-jd": "4.1.7",
    "@tarojs/plugin-platform-qq": "4.1.7",
    "@tarojs/plugin-platform-swan": "4.1.7",
    "@tarojs/plugin-platform-tt": "4.1.7",
    "@tarojs/plugin-platform-weapp": "4.1.7",
    "@tarojs/runtime": "4.1.7",
    "@tarojs/shared": "4.1.7",
    "@tarojs/taro": "4.1.7",
    "pinia": "^3.0.3",
    "vue": "^3.3.0"
  },
  "devDependencies": {
    "@babel/core": "^7.8.0",
    "@ezview/eslint-config": "^1.0.0",
    "@nutui/auto-import-resolver": "^1.0.0",
    "@tarojs/cli": "4.1.7",
    "@tarojs/taro-loader": "4.1.7",
    "@tarojs/webpack5-runner": "4.1.7",
    "@types/webpack-env": "^1.13.6",
    "@vue/babel-plugin-jsx": "^1.0.6",
    "@vue/compiler-sfc": "^3.0.0",
    "babel-preset-taro": "4.1.7",
    "cross-env": "^10.1.0",
    "css-loader": "3.4.2",
    "eslint": "^9.38.0",
    "postcss": "^8.4.18",
    "rimraf": "^6.0.1",
    "style-loader": "1.3.0",
    "stylelint": "^14.4.0",
    "tsconfig-paths-webpack-plugin": "^4.0.1",
    "typescript": "^5.1.0",
    "unplugin-vue-components": "^0.26.0",
    "vue-loader": "^17.0.0",
    "webpack": "5.94.0"
  }
}
